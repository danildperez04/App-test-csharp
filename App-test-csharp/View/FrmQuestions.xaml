        <Button Content="Respuesta 3" HorizontalAlignment="Center" Margin="0,337,0,0" VerticalAlignment="Top" Width="684" Height="41" FontFamily="Arial Narrow" FontSize="14" FontWeight="Bold" Background="#24385B" Foreground="White"/>
        <Button Content="Respuesta 4" HorizontalAlignment="Center" Margin="0,388,0,0" VerticalAlignment="Top" Width="684" Height="41" FontFamily="Arial Narrow" FontSize="14" FontWeight="Bold" Background="#24385B" Foreground="White"/>
    </Grid>
